-- Exercise 1
SELECT DISTINCT CITY FROM STATION
WHERE ID%2=0
-- Exercise2
SELECT COUNT(CITY) - COUNT(DISTINCT CITY)
AS difference
FROM STATION
-- Exercise5
SELECT candidate_id
FROM candidates
WHERE skill IN('Python', 'Tableau', 'PostgreSQL')
GROUP BY candidate_id
HAVING COUNT(DISTINCT skill) = 3 
ORDER BY candidate_id 
-- Exercise6

